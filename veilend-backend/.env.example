# VeilLend Backend - example environment variables
# Copy this file to .env and fill real secrets for production

# App
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# JWT
JWT_SECRET=your_jwt_secret_here

# Supabase (optional) - service role key recommended for server
SUPABASE_URL=
SUPABASE_KEY=
SUPABASE_SERVICE_ROLE_KEY=
USE_SUPABASE=false

# Starknet RPC / Devnet (local dev default)
STARKNET_RPC_URL=http://127.0.0.1:5050
STARKNET_DEVNET_URL=http://127.0.0.1:5050

# Admin wallet used for server-signed transactions (dev only)
ADMIN_NODE_URL=http://127.0.0.1:5050
ADMIN_WALLET_ADDRESS=0x01
ADMIN_PRIVATE_KEY=0x0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# Database fallback (local Postgres) - used if Supabase not configured
# Example: postgres://username:password@localhost:5432/veilend_dev
DATABASE_URL=postgres://postgres:postgres@localhost:5432/veilend_dev
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=veilend_dev

# Feature toggles
FALLBACK_TO_LOCAL_DB=true
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-service-role-key
JWT_SECRET=your-jwt-secret
